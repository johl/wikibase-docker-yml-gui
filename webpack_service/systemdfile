[Unit]
Description=a systemd service for webpack
Documentation=https://github.com/wmde/wikibase-docker-yml-gui
After=network.target

[Service]
Type=simple
DynamicUser=yes
ExecStart=/usr/bin/npm run dev --prefix /home/icke/Arbeit/wiki/blubber/wmde/wikibase-docker-yml-gui
Restart=on-failure

[Install]
WantedBy=multi-user.target
